  "bugs": {
    "url": "https://github.com/BLovegrove/rtfm-bot/issues"
  },
  "homepage": "https://github.com/BLovegrove/rtfm-bot#readme",
  "dependencies": {
    "discord.js": "^14.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
